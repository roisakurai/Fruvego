                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.58"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{detailResepVM.detailResepModel.txtTotalBelanjaThree}"
                        style="@style/txtInterregular12_3" tools:text="@string/msg_total_belanja_3"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtRpCounterThree"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_169pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.42"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{detailResepVM.detailResepModel.txtRpCounterThree}"
                        style="@style/txtInterbold12_3" tools:text="@string/lbl_rp_52_000"/>
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatButton
