                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_17pxh"
                        android:layout_marginStart="@dimen/_17pxh"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{detailResepVM.detailResepModel.txtBahanSteak}"
                        style="@style/txtInterbold10" tools:text="@string/lbl_bahan_steak"/>
                    <TextView android:id="@+id/txtWeight"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_22pxh"
                        android:layout_marginStart="@dimen/_22pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="wrap_content"
                        android:text="@{detailResepVM.detailResepModel.txtWeight}"
                        style="@style/txtInterbold10" tools:text="@string/msg_200_250_gram_ri"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLihatselengkap"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{detailResepVM.detailResepModel.txtLihatselengkap}"
                        style="@style/txtInterbold12_2" tools:text="@string/msg_lihat_selengkap"/>
                    <View android:background="@color/bluegray_102"
                        android:id="@+id/viewRectangle195"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_10pxv"
                        android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_360pxh"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtRekomendasiBah"
