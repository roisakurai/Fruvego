                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{detailResepVM.detailResepModel.txtBahanSelemMar}"
                        style="@style/txtInterbold10" tools:text="@string/msg_bahan_selem_mar"/>
                    <TextView android:id="@+id/txtDescription"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_22pxh"
                        android:layout_marginStart="@dimen/_22pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_width="wrap_content"
                        android:text="@{detailResepVM.detailResepModel.txtDescription}"
                        style="@style/txtInterbold10" tools:text="@string/msg_2_sendok_makan"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtBahanSteak"
